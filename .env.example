# Hewston environment variables (example)
# Copy to .env and adjust values as needed; do NOT commit real secrets.

# Backend API host/port
HEWSTON_API_HOST=127.0.0.1
HEWSTON_API_PORT=8000

# WebSocket base URL (frontend may use this)
HEWSTON_WS_URL=ws://localhost:8000

# SQLite catalog path
SQLITE_CATALOG_PATH=data/catalog.sqlite

# Databento API key (required for ingest)
DATABENTO_API_KEY=REPLACE_ME

# Python runtime tweaks
PYTHONUNBUFFERED=1

